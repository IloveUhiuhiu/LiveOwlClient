<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.client.liveowl.controller.VideoPlayerController">
    <children>


        <Button fx:id="playButton" layoutX="246.0" layoutY="360.0" onAction="#clickPlayButton" prefHeight="20.0" prefWidth="50.0" style="-fx-background-color: #FFB6AF;" text="Play" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
        <Button fx:id="pauseButton" layoutX="302.0" layoutY="360.0" onAction="#clickPauseButton" prefHeight="20.0" prefWidth="50.0" style="-fx-background-color: FAE0C7;" text="Pause" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>

      <ImageView fx:id="video" fitHeight="359.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true" />
    </children>
</AnchorPane>
