<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.client.liveowl.controller.VideoPlayerController">
    <children>


        <Button fx:id="playButton" layoutX="219.0" layoutY="360.0" text="Play" />
        <Button fx:id="pauseButton" layoutX="270.0" layoutY="360.0" text="Pause" />
        <Button fx:id="stopButton" layoutX="330.0" layoutY="360.0" text="Stop" />

      <ImageView fx:id="video" fitHeight="359.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true" />
    </children>
</AnchorPane>
